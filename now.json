{
  "version": 2,
  "name": "ls",
  "build": {
    "env": {
      "SENDGRID_API_KEY": "@sendgrid_api_key"
    }
  },
  "builds": [
    {
      "src": "/gatsbyjs/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" },
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/gatsbyjs/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
