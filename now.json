{
  "version": 2,
  "name": "ls",
  "builds": [
    {
      "src": "/gatsbyjs/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" },
      "env": {
        "VARIABLE_NAME": "sendgrid_api_key"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/gatsbyjs/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
